{
    "name": "teakowa/cryptomute",
    "description": "Encrypt numeric data (binary, decimal, hex) preserving it's format (eg. 16-digits card number to 16-digits encrypted number).",
    "keywords": ["cryptomute", "permute", "encode", "decode", "encrypt", "decrypt", "obfuscate", "format-preserving"],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Teakowa",
            "email": "packagist+contact@teakowa.me"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-gmp": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.21",
        "phpunit/phpunit": "^8.5 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cryptomute\\": "lib/Cryptomute/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cryptomute\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
